@{
                @model List<tb_login>;
                ViewData["Title"] = "Produto";

                Layout = "_Layout_dashboard"; }

<div>
    <div class="col-md-12 text-center">
        <h5>Lista de Colaboradores</h5>
    </div>
<br />

    @if (Model.Count > 0)
    {

<table class="table table-striped table-sm">
    <thead class="thead-dark">
        <tr>
            <th></th>
            <th>Código</th>
            <th>Nome</th>
            <th>Email</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td></td>
    <td>@item.CodigoLogin</td>
    <td>@item.Nome</td>
    <td>@item.Email</td>
    <td><button type="button" class="btn btn-warning btn-sm" onclick="window.location.href='/usuario/editaradmin?CodigoLogin=@item.CodigoLogin&Nome=@item.Nome&Email=@item.Email&Telefone=@item.Telefone&CPF_CNPJ=@item.CPF_CNPJ&Senha=@item.Senha&Tipo=@item.Tipo&Ativo=@item.Ativo'"><i class="fa fa-edit"></i></button></td>
</tr>}
    </tbody>
</table> }
else
{
<div class="alert alert-primary" role="alert">
    Sem Registros!
</div>}

</div>




